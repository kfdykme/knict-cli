        "knict": "^0.2.2",
        "prompt": "^1.2.0",
        "util": "^0.12.4"
    },
    "devDependencies": {
        "@types/webpack": "^5.28.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3",
        "webpack": "^5.5.0",
        "webpack-cli": "^4.8.0"
    }
}
